package ShapesLab23;

public class Etriangle extends Shape{
    private int s;
    public Etriangle(int s){
        super("ETriangle");
        this.s=s;
    }
    public double getArea(){
        return .5*Math.sqrt(s*s-((s*s)/4))*(s/2);
    }
    public double getPerimeter(){
        return s*3;
    }
}